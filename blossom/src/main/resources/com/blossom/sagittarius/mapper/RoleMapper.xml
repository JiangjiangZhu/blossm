<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.blossom.sagittarius.dao.RoleDao">
    <select id="listByUsername" resultType="Role">
        select * from role as a inner join user_role as b on a.role_id = b.role_id where b.username = #{username}
    </select>
</mapper>